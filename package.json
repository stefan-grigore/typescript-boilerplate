{
  "name": "ts-boilerplate",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "start": "tsx src/index.ts",
    "bdd": "cucumber-js",
    "bdd:cov": "c8 -o coverage -r json -r text --all --src ./src --include \"**/*.ts\" --exclude \"**/*.d.ts\" --exclude \"features/**\" --exclude \"**/features/**\" --exclude \"src/index.ts\" --exclude-after-remap npm run bdd",
    "coverage:html": "c8 report -o coverage -r html -r lcovonly --all --src ./src --include \"**/*.ts\" --exclude \"**/*.d.ts\" --exclude \"features/**\" --exclude \"**/features/**\" --exclude \"src/index.ts\" --exclude-after-remap",
    "coverage:open": "start \"\" coverage/index.html",
    "coverage:view": "npm run bdd:cov && npm run coverage:html && npm run coverage:open"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@fastify/cors": "^11.1.0",
    "@fastify/formbody": "^8.0.2",
    "@fastify/swagger": "^9.5.1",
    "@fastify/swagger-ui": "^5.2.3",
    "fastify": "^5.5.0",
    "fastify-type-provider-zod": "^5.0.3",
    "jose": "^6.0.12",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@cucumber/cucumber": "^12.1.0",
    "@cucumber/html-formatter": "^21.14.0",
    "@types/node": "^24.2.1",
    "c8": "^10.1.3",
    "cucumber-html-reporter": "^7.2.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2"
  }
}
